/**
 * 4-way Or: 
 * out = (a or b or c or d)
 */

CHIP Or4Way {
    IN a, b, c, d;
    OUT out;

    PARTS:
    Or(a=a, b=b, out=temp1);
    Or(a=temp1, b=c, out=temp2);
    Or(a=temp2, b=d, out=out);
}